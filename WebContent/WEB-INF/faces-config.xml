<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
	<managed-bean>
		<managed-bean-name>usuario</managed-bean-name>
		<managed-bean-class>dds.softpoi.DispositivoConsulta</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>login.xhtml</display-name>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/principal.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>accionBusqueda</from-outcome>
			<to-view-id>/AccAnteBusqueda.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>buscar</from-outcome>
			<to-view-id>/busqueda.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>principal.xhtml</display-name>
		<from-view-id>/principal.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>historico</from-outcome>
			<to-view-id>/historialBusqueda.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/login.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>index.xhtml</display-name>
		<from-view-id>/index.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>user</from-outcome>
			<to-view-id>/busquedaUsuario.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
